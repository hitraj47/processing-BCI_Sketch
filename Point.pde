class Point {
  
  private float x,y;
  
  public Point(float _x, float _y) {
    this.x = _x;
    this.y = _y;
  }
  
  public void setX(float _x) {
    this.x = _x;
  }
  
  public float getX() {
    return this.x;
  }
  
  public void setY(float _y) {
    this.y = _y;
  }
  
  public float getY() {
    return this.y;
  }
  
}
